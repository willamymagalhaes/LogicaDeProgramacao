programa
{
	
	
	funcao inicio()
	{
		inteiro contador = 0
	 	cadeia cesta[][]={{"João","São Paulo","(11)9999-5241"},{"Maria","Ribeirão Preto","(16)9999-8596"},{"Ana","Manaus","(92)9999-8574"}}
		
		faca{
			escreva("Nome: " + cesta[contador][0] + "   	Local: " + cesta[contador][1] + " 	 Telefone: " + cesta[contador][2] + " \n ")
			contador++
		}enquanto(contador<=3)
		
	}
}
